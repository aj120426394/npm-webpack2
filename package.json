{
  "name": "webpack2-general-config",
  "version": "1.2.4",
  "description": "This is a webpack2 configuration for gerneral purpose.",
  "main": "main.js",
  "directories": "/webpack",
  "scripts": {
    "start": "node server.js",
    "dev": "webpack-dev-server --config webpack.config.js --progress --colors",
    "build:prod": "webpack --config webpack.config.js",
    "build:dev": "webpack --config webpack.config.js",
    "test": "npm run test:lint",
    "test:jest": "jest",
    "test:lint": "npm run test:lint:js && npm run test:lint:scss",
    "test:lint:js": "eslint --config=\"./.eslintrc.yml\" ./app/**/*.jsx",
    "test:lint:scss": "stylelint \"./app/scss/components/*.scss\" --allow-empty-input",
    "deploy": "cp -r /app/source/multi-function-site/dist/ /app/public/mfs",
    "clean": "rm -r /app/public/mfs"
  },
  "keywords": [
    "webpack2",
    "config"
  ],
  "repository": "https://github.com/aj120426394/webpack2-config",
  "author": "Jafo Teng",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-airbnb": "^2.2.3",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "bower-webpack-plugin": "^0.1.9",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.14.5",
    "cssnano": "^3.10.0",
    "eslint-loader": "^1.7.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.1",
    "postcss-color-rgba-fallback": "^2.2.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-flexibility": "^1.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-opacity": "^4.0.0",
    "postcss-prefixwrap": "^1.0.0",
    "postcss-pseudoelements": "^4.0.0",
    "postcss-sorting": "^2.0.1",
    "postcss-will-change": "^1.1.0",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "sass-resources-loader": "^1.2.0",
    "style-loader": "^0.16.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0",
    "webpack-spritesmith": "^0.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "compass-mixins": "^0.12.10",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "fast-sass-loader": "^1.1.0",
    "jest-cli": "^19.0.2",
    "jquery": "^3.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-action-buffer": "^1.0.1",
    "redux-persist": "^4.5.0",
    "redux-thunk": "^2.2.0",
    "stylelint": "^7.9.0"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/webpack/**/*.js?(x)",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  }
}
