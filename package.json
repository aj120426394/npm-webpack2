{
  "name": "webpack2-general-config",
  "version": "1.0.3",
  "description": "This is a webpack2 configuration for gerneral purpose.",
  "main": "main.js",
  "directories": "/webpack",
  "scripts": {
    "start": "node server.js",
    "dev": "webpack-dev-server --config webpack.config.babel.js --progress --colors",
    "build:prod": "webpack",
    "build:dev": "webpack",
    "test": "npm run test:lint",
    "test:jest": "jest",
    "test:lint": "npm run test:lint:js && npm run test:lint:scss",
    "test:lint:js": "eslint --config=\"./.eslintrc.yml\" ./app/**/*.jsx",
    "test:lint:scss": "stylelint \"./app/scss/components/*.scss\" --allow-empty-input",
    "deploy": "cp -r /app/source/multi-function-site/dist/ /app/public/mfs",
    "clean": "rm -r /app/public/mfs"
  },
  "keywords": [
    "webpack2",
    "config"
  ],
  "repository": "https://github.com/aj120426394/webpack2-config",
  "author": "Jafo Teng",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.22.0",
    "bower-webpack-plugin": "^0.1.9",
    "clean-webpack-plugin": "^0.1.10",
    "compass-mixins": "^0.12.10",
    "css-loader": "^0.26.2",
    "cssnano": "^3.7.3",
    "eslint-loader": "^1.6.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.20.0",
    "node-sass": "^4.5.0",
    "postcss-color-rgba-fallback": "^2.2.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-flexibility": "^1.1.0",
    "postcss-loader": "^1.3.2",
    "postcss-opacity": "^4.0.0",
    "postcss-prefixwrap": "^1.0.0",
    "postcss-pseudoelements": "^3.0.0",
    "postcss-sorting": "^2.0.1",
    "postcss-will-change": "^1.1.0",
    "sass-loader": "^6.0.2",
    "sass-resources-loader": "^1.1.0",
    "style-loader": "^0.13.1",
    "stylelint-webpack-plugin": "^0.7.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.2.0",
    "webpack-merge": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "jest-cli": "^19.0.2",
    "stylelint": "^7.9.0"
  },
  "jest": {
    "testMatch": ["**/__tests__/**/*.test.js?(x)"],
    "collectCoverage": true,
    "collectCoverageFrom": ["**/webpack/**/*.js?(x)", "!**/node_modules/**", "!**/vendor/**"]
  }
}
